{
  "name": "bootstrap-components-demo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open --config ./config/webpack.config.dev.js",
    "test": "node config/scripts/test.js --env=jsdom",
    "snapshot": "jest --updateSnapshot",
    "build": "node ./config/scripts/build",
    "serve": "pushstate-server build",
    "prod": "npm-run-all --sequential build serve",
    "reboot": "npm-run-all --sequential clean yinstall --parallel start prod",
    "clean": "rimraf ./build ./node_modules",
    "yinstall": "yarn install"
  },
  "dependencies": {
    "bootstrap-components": "^1.1.218",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^2.2.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^4.0.2",
    "css-loader": "^0.26.1",
    "detect-port": "^1.1.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.17.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "filesize": "^3.5.5",
    "fs-extra": "^2.0.0",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "object-assign": "^4.1.1",
    "postcss-loader": "^0.9.1",
    "promise": "^7.1.1",
    "pushstate-server": "^2.2.1",
    "react-dev-utils": "^0.5.2",
    "react-element-to-jsx-string": "^6.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-scripts": "^0.9.2",
    "react-svg-loader": "^1.1.1",
    "react-syntax-highlighter": "^5.0.0",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "svg-url-loader": "^2.0.1",
    "uniq": "^1.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-manifest-plugin": "^1.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "homepage": "https://getreactbootstrap.com",
  "repository": "https://github.com/axiom-team/bootstrap-components",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
